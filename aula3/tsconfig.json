    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@models": ["models/index"], // Mapeia o import direto de "@models"
      "@models/*": ["models/*"], // Mapeia imports espec√≠ficos como "@models/user"
